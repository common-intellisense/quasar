{
  "name": "QScrollObserver",
  "methods": [
    {
      "name": "trigger",
      "description": "Emit a 'scroll' event",
      "params": "(immediately: Boolean) => void",
      "description_zh": "发出“滚动”事件"
    },
    {
      "name": "getPosition",
      "description": "Get current scroll details under the form of an Object: { position, direction, directionChanged, inflectionPoint }",
      "params": "",
      "description_zh": "以 Object 的形式获取当前滚动详细信息：{ position， direction， directionChanged， inflectionPoint }"
    }
  ],
  "events": [
    {
      "name": "scroll",
      "description": "Emitted when scroll position changes",
      "params": "(details: Object) => void",
      "description_zh": "滚动位置更改时发出"
    }
  ],
  "props": {
    "debounce": {
      "value": "",
      "type": "String | Number",
      "description": "Debounce amount (in milliseconds)",
      "default": "",
      "description_zh": "去抖动量（以毫秒为单位）"
    },
    "axis": {
      "value": "",
      "type": "String",
      "description": "Axis on which to detect changes",
      "default": "vertical",
      "description_zh": "用于检测更改的轴"
    },
    "scroll-target": {
      "value": "",
      "type": "String",
      "description": "",
      "default": "",
      "extends": "scroll-target",
      "description_zh": ""
    }
  },
  "link": "https://v2.quasar.dev/vue-components/scroll-observer",
  "link_zh": "https://v2.quasar.dev/vue-components/scroll-observer"
}
