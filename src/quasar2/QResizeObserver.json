{
  "name": "QResizeObserver",
  "methods": [
    {
      "name": "trigger",
      "description": "Emit a 'resize' event",
      "params": "(immediately: Boolean) => void",
      "description_zh": "发出“调整大小”事件"
    }
  ],
  "events": [
    {
      "name": "resize",
      "description": "Parent element has resized (width or height changed)",
      "params": "(size: Object) => void",
      "description_zh": "父元素已调整大小（宽度或高度已更改）"
    }
  ],
  "props": {
    "debounce": {
      "value": "",
      "type": "String | Number",
      "description": "Debounce amount (in milliseconds)",
      "default": 100,
      "description_zh": "去抖动量（以毫秒为单位）"
    }
  },
  "link": "https://v2.quasar.dev/vue-components/resize-observer",
  "link_zh": "https://v2.quasar.dev/vue-components/resize-observer"
}
